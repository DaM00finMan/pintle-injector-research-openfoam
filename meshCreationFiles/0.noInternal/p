FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 500000;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    walls
    {
        type            zeroGradient;
    }
    fuelInlet
    {
        type            zeroGradient;
    }
    oxidizerInlet
    {
        type            zeroGradient;
    }
    outlet
    {
	//type		freestream;
	//freestreamValue uniform 101325;  
	type 		fixedValue;
	value 		uniform 53903;    
	//type            zeroGradient;
        //type            totalPressure;
	//psi		thermo:psi;
	//gamma		1.192;
        //p0              uniform 529500;
        //value           uniform 529500;
    }
    axis
    {
        type            empty;
    }
}


