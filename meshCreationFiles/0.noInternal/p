FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];


internalField   uniform 51575;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    walls
    {
        type            zeroGradient;
    }
    fuelInlet
    {
        type            zeroGradient;
        //type            fixedValue;
        //value           uniform 500000;
    }
    oxidizerInlet
    {
        type            zeroGradient;
        //type            fixedValue;
        //value           uniform 1e06;
    }
    outlet
    {
	      //type		freestream;
	      //freestreamValue uniform 51575;
	      type 		  fixedValue;
        value 		uniform 51575;
    }
    axis
    {
        type            empty;
    }
}


