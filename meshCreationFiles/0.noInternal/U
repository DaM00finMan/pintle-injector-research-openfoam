FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//fuelInletVelocity       21.9042;
//oxidizerInletVelocity   8.4491;
fuelInletVelocity       713;
//oxidizerInletVelocity   40;

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (713 0 0);

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    walls
    {
        type            noSlip;
    }
    inlet
    {

        type            fixedValue;
        value           uniform ($fuelInletVelocity 0 0);

        //type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    axis
    {
        type            empty;
    }
}


