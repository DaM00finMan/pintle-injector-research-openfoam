FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
//fuelInletVelocity       21.9042;
//oxidizerInletVelocity   8.4491;
//fuelInletVelocity       100;
oxidizerInletVelocity   206;

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    walls
    {
        type            noSlip;
    }
    fuelInlet
    {
        /*
        type            fixedValue;
        value           uniform ($fuelInletVelocity 0 0);
        */
        type            noSlip;
    }
    oxidizerInlet
    {
        /*
        type            flowRateInletVelocity;
        massFlowRate    9.72e-04;
        rhoInlet        12.086; // O2 density at p=1MPa, T=300K
        //rhoInlet        6.028; // O2 density at p=0.5MPa, T=320K
        */
        /*
        type            fixedValue;
        value           uniform (0 $oxidizerInletVelocity 0);
        */
        type            noSlip;
    }
    outlet
    {
        type            zeroGradient;
    }
    axis
    {
        type            empty;
    }
}


