FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    arch        "LSB;label=32;scalar=64";
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];


internalField   uniform (0 0 0);

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    walls
    {
        type            noSlip;
    }
    inlet
    {
        type            fixedValue;
        value           uniform (21.9042 0 0);
    }
    outlet
    {
        type            zeroGradient;
    }


    master23
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    slave23
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    master345
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    slave345
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    master6897
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    slave6897
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
}


