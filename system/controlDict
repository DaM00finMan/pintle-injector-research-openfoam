FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
#include "../meshCreationFiles/controlVariables"

application     sprayFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         $EndTime;

deltaT          2.0e-06;

writeControl    adjustableRunTime;

writeInterval   1e-04;

purgeWrite      0;

writeFormat     binary;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

adjustTimeStep  yes;

maxCo           0.25;

//minDeltaT      1e-08;

runTimeModifiable yes;

functions
{
    #includeFunc Qdot
    #includeFunc residuals
